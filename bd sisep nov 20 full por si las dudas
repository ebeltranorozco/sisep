  `TIPO DE SOLICITANTE` text,
  `CESIONADO USUARIO` char(1) DEFAULT 'S',
  `idPrograma` int(11) DEFAULT NULL,
  `idComponente` int(11) DEFAULT NULL,
  `idIncentivo` int(11) DEFAULT NULL,
