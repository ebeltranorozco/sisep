  `componenteId` int(11) NOT NULL,
  `nombreDelegado` varchar(100) DEFAULT NULL,
  `correoDelegado` varchar(150) DEFAULT NULL,
  `nombreSubDelegado` varchar(100) DEFAULT NULL,
  `correoSubDelegado` varchar(150) DEFAULT NULL,
  `nombreJefePrograma1` varchar(100) DEFAULT NULL,
  `correoJefePrograma1` varchar(150) DEFAULT NULL,
  `nombreJefePrograma2` varchar(100) DEFAULT NULL,
  `correoJefePrograma2` varchar(150) DEFAULT NULL,
  `nombreJefeDistrito` varchar(100) DEFAULT NULL,
  `correoJefeDistrito` varchar(150) DEFAULT NULL,
  `nombreAuxiliar1` varchar(100) DEFAULT NULL,
  `correoAuxiliar1` varchar(150) DEFAULT NULL,
  `nombreAuxiliar2` varchar(100) DEFAULT NULL,
  `correoAuxiliar2` varchar(150) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

