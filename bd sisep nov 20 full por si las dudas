-- Volcando estructura para vista sagarpa.listadoOrganismo
-- Creando tabla temporal para superar errores de dependencia de VIEW
CREATE TABLE `listadoOrganismo` (
	`id` INT(11) NOT NULL,
	`razonSocial` VARCHAR(100) NOT NULL COLLATE 'utf8_general_ci',
	`nombreCorto` VARCHAR(30) NOT NULL COLLATE 'utf8_general_ci',
	`rfc` VARCHAR(12) NOT NULL COLLATE 'utf8_general_ci',
	`calle` VARCHAR(200) NOT NULL COLLATE 'utf8_general_ci',
	`numExterior` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`numInterior` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`colonia` VARCHAR(100) NOT NULL COLLATE 'utf8_general_ci',
	`localidad` VARCHAR(100) NOT NULL COLLATE 'utf8_general_ci',
	`idMunicipio` INT(11) NOT NULL,
	`estado` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`sitioWeb` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`regimen` VARCHAR(200) NOT NULL COLLATE 'utf8_general_ci',
	`numCsd` VARCHAR(30) NOT NULL COLLATE 'utf8_general_ci',
	`vencimiento` DATE NOT NULL,
	`ruta_csd` VARCHAR(255) NOT NULL COLLATE 'utf8_general_ci',
	`impuestoIva` DECIMAL(14,2) NULL,
	`pais` VARCHAR(45) NULL COLLATE 'utf8_general_ci',
	`codigoPostal` VARCHAR(45) NULL COLLATE 'utf8_general_ci',
	`logoCfdi` VARCHAR(150) NULL COLLATE 'utf8_general_ci',
	`colorBarra` VARCHAR(45) NULL COLLATE 'utf8_general_ci',
	`telefono` VARCHAR(30) NOT NULL COLLATE 'utf8_general_ci',
	`email` VARCHAR(100) NOT NULL COLLATE 'utf8_general_ci',
	`estatusId` INT(11) NOT NULL,
	`estatus` VARCHAR(45) NOT NULL COLLATE 'utf8_general_ci'
) ENGINE=MyISAM;

