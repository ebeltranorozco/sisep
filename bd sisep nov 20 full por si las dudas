	(9, 'RIEGO PM SIN CENSION', 2017, 36, 3658, 1, 'M', 'N', '<div>\n<p style="margin-left:0cm; margin-right:0cm"><span style="color:#000000"><span style="font-family:Times New Roman"><span style="font-size:small">&nbsp;</span></span></span></p>\n\n<table border="1" cellspacing="0" style="border-collapse:collapse; border:1pt solid windowtext; margin:auto auto auto -1.7pt">\n	<tbody>\n		<tr>\n			<td style="height:28.1pt; vertical-align:top; width:185.75pt">\n			<p style="margin-left:0cm; margin-right:0cm"><span style="color:#000000"><span style="font-family:Times New Roman"><span style="font-size:small">&nbsp;</span></span></span></p>\n\n			<p style="margin-left:0cm; margin-right:0cm"><span style="color:#000000"><span style="font-family:Times New Roman"><span style="font-size:medium"><img src="https://upload.wikimedia.org/wikipedia/commons/thumb/1/1b/SAGARPA_logo_2012.svg/1200px-SAGARPA_logo_2012.svg.png" style="height:200px; width:200px" /></span></span></span></p>\n\n			<p style="margin-left:0cm; margin-right:0cm"><span style="color:#000000"><span style="font-family:Times New Roman"><span style="font-size:small">&nbsp;</span></span></span></p>\n\n			<p style="margin-left:0cm; margin-right:0cm"><span style="color:#000000"><span style="font-family:Times New Roman"><span style="font-size:small">&nbsp;</span></span></span></p>\n\n			<p style="margin-left:0cm; margin-right:0cm"><span style="color:#000000"><span style="font-family:Times New Roman"><span style="font-size:small">&nbsp;</span></span></span></p>\n\n			<p style="margin-left:0cm; margin-right:0cm"><span style="color:#000000"><span style="font-family:Times New Roman"><span style="font-size:small">&nbsp;</span></span></span></p>\n\n			<p style="margin-left:0cm; margin-right:0cm"><span style="color:#000000"><span style="font-family:Times New Roman"><span style="font-size:small">&nbsp;</span></span></span></p>\n			</td>\n			<td style="border-color:windowtext windowtext windowtext #000000; height:28.1pt; vertical-alig