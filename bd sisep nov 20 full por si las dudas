  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `nombre` varchar(150) DEFAULT NULL,
  `nombreUsuario` varchar(20) NOT NULL,
  `contrasena` text NOT NULL,
  `email` varchar(100) DEFAULT NULL,
  `empleadoId` int(11) NOT NULL,
