CREATE ALGORITHM=UNDEFINED DEFINER=`dbsagarpa`@`%` SQL SECURITY DEFINER VIEW `sisepListadoRiegoGlobal` AS select `a`.`id` AS `id`,`a`.`fechaVentanilla` AS `fechaVentanilla`,`a`.`noRemesa` AS `noRemesa`,`a`.`noFolioInterno` AS `noFolioInterno`,`a`.`folioSuri` AS `folioSuri`,`a`.`fechaFolioSuri` AS `fechaFolioSuri`,`a`.`nombreSolicitante` AS `nombreSolicitante`,`a`.`conceptoInversionSolicitado` AS `conceptoInversionSolicitado`,`a`.`has` AS `has`,`a`.`ddr` AS `ddr`,`a`.`apoyoSolicitado` AS `apoyoSolicitado`,`a`.`aportacionProductor` AS `aportacionProductor`,`a`.`inversionTotal` AS `inversionTotal`,`a`.`cultivosAtender` AS `cultivosAtender`,`a`.`nombreProyecto` AS `nombreProyecto`,`a`.`nombreReceptorVentanilla` AS `nombreReceptorVentanilla`,`a`.`tipoSolicitante` AS `tipoSolicitante`,`a`.`curp` AS `curp`,`a`.`rfc` AS `rfc`,`a`.`correoElectronico` AS `correoElectronico`,`a`.`telefono` AS `telefono`,`a`.`nombreRepresentanteLegal` AS `nombreRepresentanteLegal`,`a`.`curpRepresentanteLegal` AS `curpRepresentanteLegal`,`a`.`rfcRepresentanteLegal` AS `rfcRepresentanteLegal`,`a`.`estadoProyecto` AS `estadoProyecto`,`a`.`municipioProyecto` AS `municipioProyecto`,`a`.`localidadProyecto` AS `localidadProyecto`,`a`.`programa` AS `programa`,`a`.`componente` AS `componente`,`a`.`componenteId` AS `componenteId`,`a`.`incentivo` AS `incentivo`,`a`.`descripcionProyecto` AS `descripcionProyecto`,`a`.`coordenadasPredio` AS `coordenadasPredio`,`a`.`anoEjercicio` AS `anoEjercicio`,`a`.`estatusId` AS `estatusId`,`b`.`nombre` AS `estatus` from (`sisepRiegoGlobal` `a` join `estatus` `b` on((`b`.`id` = `a`.`estatusId`)));

-- Volcando estructura para vista sagarpa.sisepListadoSeguimientoRiegoEnc
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `sisepListadoSeguimientoRiegoEnc`;
CREATE ALGORITHM=UNDEFINED DEFINER=`dbsagarpa`@`%` SQL SECURITY DEFINER VIEW `sisepListadoSeguimientoRiegoEnc` AS select `a`.`id` AS `id`,`a`.`noOficio` AS `noOficio`,date_format(`a`.`fechaOficio`,'%d-%m-%Y') AS `fechaOficio`,date_format(`a`.`fechaAcuse`,'%d-%m-%Y') AS `fechaAcuse`,`b`.`nombre` AS `estatus`,`c`.`nombre` AS `estatusSegimiento` from ((`sisepSeguimientoRiegoEnc` `a` join `estatus` `b` on((`b`.`id` = `a`.`estatusId`))) join `estatusSeguimiento` `c` on((`c`.`id` = `a`.`estatusSeguimientoId`))) order by `a`.`id` desc;

-- Volcando estructura para vista sagarpa.sisepListadoUbicacionProyecto
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `sisepListadoUbicacionProyecto`;
CREATE ALGORITHM=UNDEFINED DEFINER=`dbsagarpa`@`%` SQL SECURITY DEFINER VIEW `sisepListadoUbicacionProyecto` AS select `a`.`id` AS `id`,`a`.`idPadronBeneficiario` AS `idPadronBeneficiario`,`a`.`tipoDomicilioProyecto` AS `tipoDomicilioProyecto`,`a`.`tipoAsentamientoProyecto` AS `tipoAsentamientoProyecto`,`a`.`localidadProyecto` AS `localidadProyecto`,`a`.`nombreAsentamientoProyecto` AS `nombreAsentamientoProyecto`,`a`.`tipoVialidadProyecto` AS `tipoVialidadProyecto`,`a`.`nombreVialidadProyecto` AS `nombreVialidadProyecto`,`a`.`cpProyecto` AS `cpProyecto`,`a`.`referencia1Proyecto` AS `referencia1Proyecto`,`a`.`referencia2Proyecto` AS `referencia2Proyecto`,`a`.`estadoProyecto` AS `estadoProyecto`,`a`.`municipioProyecto` AS `municipioProyecto`,`a`.`totalHectareasProyecto` AS `totalHectareasProyecto`,`a`.`hectareasProyecto` AS `hectareasProyecto`,`a`.`coordenadasNProyecto` AS `coordenadasNProyecto`,`a`.`coordenadasWProyecto` AS `coordenadasWProyecto`,`a`.`acreditacionPropiedadProyecto` AS `acreditacionPropiedadProyecto`,`a`.`folioAcreditacionPropiedadProyecto` AS `folioAcreditacionPropiedadProyecto`,`a`.`estatus` AS `estatus`,`b`.`nombreSolicitante` AS `nombreSolicitante` from (`sisepUbicacionProyecto` `a` join `sisepRiegoGlobal` `b` on((`b`.`id` = `a`.`idPadronBeneficiario`)));

