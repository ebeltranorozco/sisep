-- Volcando estructura para tabla sagarpa.empleado
CREATE TABLE IF NOT EXISTS `empleado` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `numeroEmpleadoSistemaNominas` int(11) DEFAULT NULL,
  `nombre` varchar(100) NOT NULL,
  `apellidoPaterno` varchar(100) NOT NULL,
  `apellidoMaterno` varchar(100) DEFAULT NULL,
  `rfc` varchar(13) DEFAULT NULL,
  `curp` varchar(25) DEFAULT NULL,
  `fechaNacimiento` date DEFAULT NULL,
  `fechaIngreso` date DEFAULT NULL,
  `telefono` varchar(50) DEFAULT NULL,
  `email` varchar(100) DEFAULT NULL,
  `sueldoBruto` double(14,4) DEFAULT NULL,
  `idOrganismo` int(11) DEFAULT NULL,
  `idUsuario` int(11) NOT NULL,
  `idAdscripcion` int(11) DEFAULT NULL,
  `idCampana` int(11) DEFAULT NULL,
  `idPuesto` int(11) DEFAULT NULL,
  `calle` varchar(200) DEFAULT NULL,
  `numExterior` varchar(45) DEFAULT NULL,
  `colonia` varchar(100) DEFAULT NULL,
  `codigoPostal` varchar(10) DEFAULT NULL,
  `localidad` varchar(100) DEFAULT NULL,
  `municipio` varchar(80) DEFAULT NULL,
  `estado` varchar(80) DEFAULT NULL,
  `lugarNacimiento` varchar(100) DEFAULT NULL,
  `paisNacimiento` varchar(100) DEFAULT NULL,
  `sexo` varchar(1) DEFAULT NULL,
  `idEstadoCivil` int(11) DEFAULT NULL,
  `numeroSeguridadSocial` varchar(45) DEFAULT NULL,
  `conyuge` varchar(100) DEFAULT NULL,
  `nombrePadre` varchar(100) DEFAULT NULL,
  `nombreMadre` varchar(100) DEFAULT NULL,
  `idPuestoalqueReporta` int(11) DEFAULT NULL,
  `idPuestoqueOcupa` int(11) DEFAULT NULL,
  `descripcionFunciones` varchar(500) DEFAULT NULL,
  `beneficiarioIncapacidad` varchar(100) DEFAULT NULL,
  `beneficiarioIncapacidadParentesco` varchar(45) DEFAULT NULL,
  `numEmpleado` varchar(45) DEFAULT NULL,
  `uuidEmpleado` varchar(50) DEFAULT NULL,
  `edad` int(3) DEFAULT NULL,
  `numeroActa` int(6) DEFAULT NULL,
  `ano` int(4) DEFAULT NULL,
  `foja` int(4) DEFAULT NULL,
  `libro` int(4) DEFAULT NULL,
  `fechaNacimientoPadre` date DEFAULT NULL,
  `fechaNacimientoMadre` date DEFAULT NULL,
  `fechaNacimientoConyuga` date DEFAULT NULL,
  `celular` varchar(10) DEFAULT NULL,
  `homoclave` varchar(50) DEFAULT NULL,
  `folioElector` varchar(50) DEFAULT NULL,
  `numeroLicencia` varchar(50) DEFAULT NULL,
  `pantalonHombre` int(3) DEFAULT NULL,
  `camisa` int(2) DEFAULT NULL,
  `zapatos` varchar(10) DEFAULT NULL,
  `pantalonMujer` int(3) DEFAULT NULL,
  `blusa` int(2) DEFAULT NULL,
  `cartillaMilitar` varchar(50) DEFAULT NULL,
  `numeroHijos` int(2) DEFAULT NULL,
  `pais` varchar(45) DEFAULT 'MEXICO',
  `otrosEstudios` tinyint(1) DEFAULT '0',
  `descripcionOtrosEstudios` text,
  `estatus` tinyint(1) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`),
  KEY `fk_empleado_organismo1_idx` (`idOrganismo`),
  KEY `fk_empleado_adscripcion1_idx` (`idAdscripcion`),
  KEY `fk_empleado_campana1_idx` (`idCampana`),
  KEY `fk_empleado_puesto1_idx` (`idPuesto`),
  KEY `fk_empleado_usuario1_idx` (`idUsuario`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8 ROW_FORMAT=COMPACT;

