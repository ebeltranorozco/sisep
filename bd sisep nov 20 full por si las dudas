-- Volcando estructura para tabla sagarpa.usuario
CREATE TABLE IF NOT EXISTS `usuario` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `nombre_usuario` varchar(30) DEFAULT NULL,
  `password` varchar(90) DEFAULT NULL,
  `nombre` varchar(100) DEFAULT NULL,
  `apellidos` varchar(100) NOT NULL,
  `email` varchar(100) NOT NULL,
  `organismo_id` int(11) NOT NULL,
  `puesto_id` int(11) NOT NULL,
  `estatus_id` int(11) NOT NULL,
  `es_admin` varchar(1) NOT NULL DEFAULT 'N',
  `sincronizar_basedatos_movil_remota` varchar(1) NOT NULL DEFAULT 'N',
  `minutos_sesion_inactiva` tinyint(4) NOT NULL,
  `tiempo_ultima_peticion` datetime NOT NULL,
  `token` varchar(50) NOT NULL,
  `adscripcion_id` int(11) DEFAULT NULL,
  `rastro_id` int(11) DEFAULT NULL,
  `imei` varchar(45) DEFAULT NULL,
  `role` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `nombre_usuario` (`nombre_usuario`),
  KEY `fk_usuario_organismo1_idx` (`organismo_id`),
  KEY `fk_usuario_puesto1_idx` (`puesto_id`),
  KEY `fk_usuario_estatus1_idx` (`estatus_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

-- Volcando datos para la tabla sagarpa.usuario: ~0 rows (aproximadamente)
