-- Volcando estructura para vista sagarpa.listadoOrganismo
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `listadoOrganismo`;
CREATE ALGORITHM=UNDEFINED DEFINER=`dbsagarpa`@`%` SQL SECURITY DEFINER VIEW `listadoOrganismo` AS select `a`.`id` AS `id`,`a`.`razonSocial` AS `razonSocial`,`a`.`nombreCorto` AS `nombreCorto`,`a`.`rfc` AS `rfc`,`a`.`calle` AS `calle`,`a`.`numExterior` AS `numExterior`,`a`.`numInterior` AS `numInterior`,`a`.`colonia` AS `colonia`,`a`.`localidad` AS `localidad`,`a`.`idMunicipio` AS `idMunicipio`,`a`.`estado` AS `estado`,`a`.`sitioWeb` AS `sitioWeb`,`a`.`regimen` AS `regimen`,`a`.`numCsd` AS `numCsd`,`a`.`vencimiento` AS `vencimiento`,`a`.`ruta_csd` AS `ruta_csd`,`a`.`impuestoIva` AS `impuestoIva`,`a`.`pais` AS `pais`,`a`.`codigoPostal` AS `codigoPostal`,`a`.`logoCfdi` AS `logoCfdi`,`a`.`colorBarra` AS `colorBarra`,`a`.`telefono` AS `telefono`,`a`.`email` AS `email`,`a`.`estatusId` AS `estatusId`,`b`.`nombre` AS `estatus` from (`organismo` `a` join `estatus` `b` on((`b`.`id` = `a`.`estatusId`)));

-- Volcando estructura para vista sagarpa.listadoPrograma
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `listadoPrograma`;
CREATE ALGORITHM=UNDEFINED DEFINER=`dbsagarpa`@`%` SQL SECURITY DEFINER VIEW `listadoPrograma` AS select `a`.`id` AS `id`,`a`.`nombre` AS `nombre`,`a`.`descripcion` AS `descripcion`,`a`.`noPrograma` AS `noPrograma`,`a`.`estatusId` AS `estatusId`,`b`.`nombre` AS `estatus` from (`programa` `a` join `estatus` `b` on((`b`.`id` = `a`.`estatusId`)));

-- Volcando estructura para vista sagarpa.listadoSisepCatCargaMasiva
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `listadoSisepCatCargaMasiva`;
CREATE ALGORITHM=UNDEFINED DEFINER=`dbsagarpa`@`%` SQL SECURITY DEFINER VIEW `listadoSisepCatCargaMasiva` AS select `carMas`.`id` AS `id`,`carMas`.`nombreProceso` AS `nombreProceso`,`carMas`.`campos` AS `campos`,`carMas`.`estatusId` AS `estatusId`,`est`.`nombre` AS `estatus` from (`sisepCatCargaMasiva` `carMas` join `estatus` `est` on((`est`.`id` = `carMas`.`estatusId`)));

