  `coordenadasWProyecto` varchar(100) DEFAULT NULL,
  `acreditacionPropiedadProyecto` varchar(100) DEFAULT NULL,
  `folioAcreditacionPropiedadProyecto` varchar(100) DEFAULT NULL,
