  `puestoId` int(11) DEFAULT NULL,
  `organismoId` int(11) DEFAULT NULL,
  `sexoId` int(11) DEFAULT NULL,
  `componenteId` int(11) DEFAULT NULL,
  `tareasFrecuentes` text,
  `imagen` blob,
  `estatusId` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=18 DEFAULT CHARSET=utf8 ROW_FORMAT=COMPACT;

