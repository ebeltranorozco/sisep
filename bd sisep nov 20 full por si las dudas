  `idConcepto` int(11) DEFAULT NULL,
  `HAS SURI` double DEFAULT NULL,
  `idDDR` int(11) DEFAULT NULL,
  `APOYO SOLICITADO` double DEFAULT NULL,
  `APORTACION DEL PRODUCTOR` double DEFAULT NULL,
  `INVERSION TOTAL` double DEFAULT NULL,
  `PRODUCTO ATENDER` text,
  `NOMBRE DEL PROYECTO` text,
