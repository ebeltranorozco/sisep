  `cesionado_usuario` char(1) CHARACTER SET utf16 COLLATE utf16_spanish_ci NOT NULL DEFAULT 'S',
  `convenio` longtext CHARACTER SET utf16 COLLATE utf16_spanish_ci NOT NULL,
  PRIMARY KEY (`id_plantilla`)
) ENGINE=InnoDB AUTO_INCREMENT=16 DEFAULT CHARSET=utf8 COLLATE=utf8_spanish2_ci ROW_FORMAT=COMPACT;

