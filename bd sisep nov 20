-- Volcando estructura para tabla sisep.siseppersonasmorales
CREATE TABLE IF NOT EXISTS `siseppersonasmorales` (
  `﻿idPersonaMoral` int(11) NOT NULL AUTO_INCREMENT,
  `idPadronBeneficiario` int(11) NOT NULL,
  `nombrePersonaMoral` varchar(100) DEFAULT NULL,
  `rfcPersonaMoral` varchar(15) DEFAULT NULL,
  `callePersonaMoral` varchar(150) DEFAULT NULL,
  `numeroPersonaMoral` int(11) DEFAULT NULL,
  `coloniaPersonaMoral` varchar(150) DEFAULT NULL,
  `cpPersonaMoral` tinyint(5) DEFAULT NULL,
  `localidadPersonaMoral` varchar(100) DEFAULT NULL,
  `municipioPersonaMoral` varchar(100) DEFAULT NULL,
  `estadoPersonaMoral` varchar(50) DEFAULT NULL,
  `telefonoOficinaPersonaMoral` varchar(20) DEFAULT NULL,
  `correoPersonaMoral` varchar(70) DEFAULT NULL,
