-- Volcando estructura para tabla sisep.siseppadronbeneficiarios
CREATE TABLE IF NOT EXISTS `siseppadronbeneficiarios` (
  `id_padron_beneficiario` int(11) NOT NULL,
  `fecha_ventanilla` datetime DEFAULT NULL,
  `remesa_no` text CHARACTER SET utf8,
  `folio_interno` text CHARACTER SET utf8,
  `folio_suri` text CHARACTER SET utf8,
  `fecha_folio_suri` datetime DEFAULT NULL,
  `nombre_solicitante` text CHARACTER SET utf8,
  `id_concepto` int(11) DEFAULT NULL,
  `has_suri` double DEFAULT NULL,
  `id_ddr` int(11) DEFAULT NULL,
  `apoyo_solicitado` double DEFAULT NULL,
  `aportacion_productor` double DEFAULT NULL,
  `inversion_total` double DEFAULT NULL,
  `producto_atender` text CHARACTER SET utf8,
  `nombre_proyecto` text CHARACTER SET utf8,
  `id_usuario` int(11) DEFAULT NULL,
  `tipo_solicitante` text CHARACTER SET utf8,
  `cesionado_usuario` char(1) CHARACTER SET utf8 DEFAULT 'S',
  `id_programa` int(11) DEFAULT NULL,
  `id_componente` int(11) DEFAULT NULL,
  `id_incentivo` int(11) DEFAULT NULL,
  `descripcion_proyecto` text CHARACTER SET utf8,
  `id_ano_fiscal` int(11) DEFAULT NULL,
  `id_delegacion` int(11) DEFAULT '1',
  PRIMARY KEY (`id_padron_beneficiario`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish2_ci ROW_FORMAT=COMPACT;

