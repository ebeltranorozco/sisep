-- Volcando estructura para tabla sisep.ddrs_federales
CREATE TABLE IF NOT EXISTS `ddrs_federales` (
  `id_ddr` int(11) NOT NULL,
  `nombre_ddr` varchar(50) DEFAULT NULL,
  `jefe_ddr` varchar(50) DEFAULT NULL,
  `encargado_ddr` varchar(50) DEFAULT NULL,
  `direccion_ddr` varchar(50) DEFAULT NULL,
  `telefono1_ddr` varchar(50) DEFAULT NULL,
  `telefono2_ddr` varchar(50) DEFAULT NULL,
  `correo1_ddr` varchar(50) DEFAULT NULL,
  `correo2_ddr` varchar(50) DEFAULT NULL,
  `estado_ddr` varchar(50) DEFAULT NULL,
  `municipio_ddr` varchar(50) DEFAULT NULL,
  `localidad_ddr` varchar(50) DEFAULT NULL,
  `abreviatura_titulo_ddr` char(10) DEFAULT 'LIC.',
  `status_ddr` int(11) DEFAULT NULL,
  PRIMARY KEY (`id_ddr`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=COMPACT;

