-- Volcando estructura para tabla sisep.siseppersonasfisicas
CREATE TABLE IF NOT EXISTS `siseppersonasfisicas` (
  `id_persona_fisica` int(11) NOT NULL AUTO_INCREMENT,
  `id_padron_beneficiario` int(11) NOT NULL,
  `ap_paterno_persona_fisica` varchar(100) DEFAULT NULL,
  `ap_materno_persona_fisica` varchar(100) DEFAULT NULL,
  `nombre_persona_fisica` varchar(100) DEFAULT NULL,
  `rfc_persona_fisica` varchar(15) DEFAULT NULL,
  `curp_persona_fisica` varchar(30) DEFAULT NULL,
  `nacionalidad_persona_fisica` varchar(100) DEFAULT NULL,
  `fecha_nacimiento_persona_fisica` datetime DEFAULT NULL,
  `estado_civil_persona_fisica` varchar(20) DEFAULT NULL,
  `calle_persona_fisica` varchar(150) DEFAULT NULL,
  `numero_persona_fisica` varchar(20) DEFAULT NULL,
  `colonia_persona_fisica` varchar(150) DEFAULT NULL,
  `cp_persona_fisica` tinyint(5) DEFAULT NULL,
  `localidad_persona_fisica` varchar(100) DEFAULT NULL,
  `municipio_persona_fisica` varchar(100) DEFAULT NULL,
  `estado_persona_fisica` varchar(50) DEFAULT NULL,
  `telefono_casa_persona_fisica` varchar(20) DEFAULT NULL,
  `num_celular_persona_fisica` varchar(20) DEFAULT NULL,
  `correo_persona_fisica` varchar(70) DEFAULT NULL,
  `id_oficial_persona_fisica` varchar(50) DEFAULT NULL,
  `num_id_oficial_persona_fisica` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`id_persona_fisica`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

-- Volcando datos para la tabla sisep.siseppersonasfisicas: ~0 rows (aproximadamente)
DELETE FROM `siseppersonasfisicas`;
/*!40000 ALTER TABLE `siseppersonasfisicas` DISABLE KEYS */;
